{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "barlito/utils": "^1.0.2",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.13.3",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.13.3",
        "easycorp/easyadmin-bundle": "^4.4",
        "friendsofphp/php-cs-fixer": "^3.11",
        "friendsofsymfony/rest-bundle": "^3.4",
        "jms/serializer-bundle": "^4.2",
        "nelmio/cors-bundle": "^2.2",
        "pdepend/pdepend": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpmd/phpmd": "^2.13",
        "squizlabs/php_codesniffer": "^3.7.1",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/amqp-messenger": "6.1.*",
        "symfony/console": "6.1.*",
        "symfony/dependency-injection": "6.1.*",
        "symfony/discord-notifier": "6.1.*",
        "symfony/doctrine-messenger": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/expression-language": "6.1.*",
        "symfony/flex": "^1.19.3",
        "symfony/framework-bundle": "6.1.*",
        "symfony/lock": "6.1.*",
        "symfony/messenger": "6.1.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.1.*",
        "symfony/property-access": "6.1.*",
        "symfony/property-info": "6.1.*",
        "symfony/proxy-manager-bridge": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/security-bundle": "6.1.*",
        "symfony/serializer": "6.1.*",
        "symfony/translation": "6.1.*",
        "symfony/uid": "6.1.*",
        "symfony/validator": "6.1.*",
        "symfony/yaml": "6.1.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    },
    "require-dev": {
        "behat/behat": "^3.11",
        "doctrine/doctrine-fixtures-bundle": "^3.4.2",
        "friends-of-behat/mink": "^1.10",
        "friends-of-behat/mink-browserkit-driver": "^1.6.1",
        "friends-of-behat/mink-extension": "^2.7.1",
        "friends-of-behat/symfony-extension": "^2.3.1",
        "guzzlehttp/psr7": "^2.4.1",
        "hautelook/alice-bundle": "^2.11",
        "justinrainbow/json-schema": "^5.2.12",
        "php-http/curl-client": "^2.2.1",
        "phpunit/phpunit": "^9.5.25",
        "soyuka/contexts": "dev-main",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/http-client": "6.1.*",
        "symfony/maker-bundle": "^1.47",
        "symfony/phpunit-bridge": "6.1.*",
        "ubirak/rest-api-behat-extension": "^8.2"
    },
    "repositories": {
        "barlito/utils": {
            "type": "vcs",
            "url": "https://github.com/barlito/utils"
        }
    }
}
