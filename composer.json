{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^4.0.22",
        "barlito/utils": "^1.4.0",
        "brick/money": "^0.8.1",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^2.20.2",
        "easycorp/easyadmin-bundle": "^4.24.4",
        "friendsofphp/php-cs-fixer": "^3.70.2",
        "knpuniversity/oauth2-client-bundle": "^2.18.3",
        "lexik/jwt-authentication-bundle": "^3.1.1",
        "nelmio/cors-bundle": "^2.5.0",
        "pdepend/pdepend": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpdoc-parser": "^1.33.0",
        "squizlabs/php_codesniffer": "^3.11.3",
        "stof/doctrine-extensions-bundle": "^1.13",
        "symfony/amqp-messenger": "6.4.*",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/discord-notifier": "6.4.*",
        "symfony/doctrine-messenger": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^2.5",
        "symfony/framework-bundle": "6.4.*",
        "symfony/lock": "6.4.*",
        "symfony/messenger": "6.4.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/uid": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/yaml": "6.4.*",
        "wohali/oauth2-discord-new": "^1.2.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "require-dev": {
        "behat/behat": "^3.19",
        "doctrine/doctrine-fixtures-bundle": "^3.7.1",
        "friends-of-behat/mink": "^1.11",
        "friends-of-behat/mink-browserkit-driver": "^1.6.2",
        "friends-of-behat/mink-extension": "^2.7.5",
        "friends-of-behat/symfony-extension": "^2.6.0",
        "guzzlehttp/psr7": "^2.7.0",
        "hautelook/alice-bundle": "^2.13",
        "justinrainbow/json-schema": "^5.3.0",
        "phpunit/phpunit": "^9.6.22",
        "rector/rector": "^0.18.13",
        "soyuka/contexts": "dev-main",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/maker-bundle": "^1.62.1",
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "ubirak/rest-api-behat-extension": "^8.3"
    },
    "repositories": {
        "barlito/utils": {
            "type": "vcs",
            "url": "https://github.com/barlito/utils"
        }
    }
}
